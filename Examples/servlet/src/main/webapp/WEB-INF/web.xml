<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	version="3.0">
	<display-name>Archetype Created Web Application</display-name>
	<servlet>
		<servlet-name>SynchronousServlet</servlet-name>
		<servlet-class>com.example.servlet.SynchronousServlet</servlet-class>
		<async-supported>false</async-supported>
	</servlet>
	<servlet-mapping>
		<servlet-name>SynchronousServlet</servlet-name>
		<url-pattern>/synchronousservlet</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>AsyncServlet</servlet-name>
		<servlet-class>com.example.servlet.AsyncServlet</servlet-class>
		<async-supported>true</async-supported>
	</servlet>
	<servlet-mapping>
		<servlet-name>AsyncServlet</servlet-name>
		<url-pattern>/asyncservlet</url-pattern>
	</servlet-mapping>
</web-app>
